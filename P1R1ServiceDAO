package mil.dtic.cbes.submissions.dao;

import java.util.HashMap;
import java.util.List;

import mil.dtic.cbes.submissions.ValueObjects.Mapping;

/**
 * Service DAO to update and load P1 and R1 mappings and data.
 *
 * @class P1R1ServiceDao
 */
public interface P1R1ServiceDao extends BudgesBaseDAO<Mapping> {

  public HashMap<String, String> loadMapping(int type);
  public void updateP1R1(int p1r1Type, List<String> mapHeaderValues);
}
